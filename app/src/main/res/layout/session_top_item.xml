<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <ImageView
        android:id="@+id/list_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_session_top_bar"/>

    <TextView
        android:id="@+id/date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_octagon_session_top"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginTop="15dp"/>

    <TextView
        android:id="@+id/session_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_octagon_session_top"
        android:layout_toRightOf="@+id/date"
        android:layout_toEndOf="@+id/date"
        android:layout_alignBaseline="@+id/date"
        android:layout_marginLeft="10dp"
        />
    <TextView
        android:id="@+id/session_template"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_octagon_session_top"
        android:layout_below="@+id/date"
        android:layout_alignLeft="@+id/date"
        android:layout_marginTop="5dp"/>
    <TextView
        android:id="@+id/user_weight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_octagon_session_top"
        android:layout_alignLeft="@+id/session_num"
        android:layout_alignStart="@+id/session_num"
        android:layout_alignBaseline="@+id/session_template"
        android:layout_marginTop="10dp"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/session_num"
        android:layout_marginLeft="8dp"
        android:orientation="horizontal">

